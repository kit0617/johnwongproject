<html>
    <head> 
        <title><%=title%></title>
	
	
    </head>
    <body>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyCUjvRnevm2EQJRDYyS_Ab2BMI09001K7M"></script>
        
        <script type="text/javascript">

            var loadMap = function() 
            {
				var myLatLng = {lat: <%=lat%>, lng: <%=lon%>};
                		var map = new google.maps.Map(document.getElementById("map"),{
				      zoom: <%=zoom%>,
				      center: myLatLng
				}); 
				var marker = new google.maps.Marker({
					position: myLatLng,
					map: map,
					title: '<%=title%>'
				});
            };
            window.onload= loadMap;
        </script>
		<aside>
        	<div id="map" style="width:100%;height:100%;"></div>
		</aside>

    </body>
</html>
